{
  "name": "react-palm",
  "version": "0.8.1",
  "description": "Elm-like architecture for React apps",
  "main": "dist/src",
  "homepage": "https://github.com/btford/react-palm",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build-example": "npm run build && webpack dist/example/main.js dist/example/main.bundle.js",
    "test": "npm run build && ava",
    "flow": "flow && node ./scripts/perfect-coverage.js",
    "watch": "node bin/watch.js"
  },
  "keywords": [
    "react",
    "redux",
    "elm",
    "tasks",
    "side-effects"
  ],
  "author": "Brian Ford <btford@umich.edu>",
  "repository": {
    "type": "git",
    "url": "https://github.com/btford/react-palm.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^15.0.30",
    "@types/react-dom": "^15.5.0",
    "ava": "^0.15.2",
    "chalk": "^1.1.3",
    "enzyme": "^2.4.1",
    "flow-bin": "^0.48.0",
    "jsdom": "^9.4.2",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.5.2",
    "sinon": "^1.17.4",
    "split": "^1.0.0",
    "through2": "^2.0.1",
    "ts-loader": "^1.3.3",
    "typescript": "2.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "0.14.x - 16.x",
    "react-dom": "0.14.x - 16.x"
  },
  "ava": {
    "files": [
      "dist/test",
      "dist/example/test"
    ],
    "require": [
      "./bin/setup-browser-env.js"
    ]
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  }
}
